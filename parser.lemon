}

xx_class_property_definition(R) ::= COMMENT(C) PUBLIC IDENTIFIER(I) ASSIGN xx_literal_expr(E) DOTCOMMA . {
	R = xx_ret_class_property(XX_T_PUBLIC, I, E, C);
}

xx_class_property_definition(R) ::= PROTECTED IDENTIFIER(I) DOTCOMMA . {
